@InBook{	  heath-1999,
  author	= "Heath, Michael T. and Raghavan, Padma",
  editor	= "Heath, Michael T. and Ranade, Abhiram and Schreiber,
		  Robert S.",
  title		= "Performance of Parallel Sparse Triangular Solution",
  booktitle	= "Algorithms for Parallel Processing",
  year		= "1999",
  optpublisher	= "Springer New York",
  optaddress	= "New York, NY",
  pages		= "289--305",
  abstract	= "Many applications require the solution of a sequence of
		  sparse linear systems with the same matrix but different
		  right-hand-side vectors. When the matrix is very large and
		  sparse, the sparse matrix factors are computed in parallel
		  and used in forward and back substitution steps to compute
		  the solution. These substitution steps are not performed at
		  high efficiency on message-passing multiprocessors and pose
		  a performance bottleneck in applications requiring repeated
		  solutions. In this paper we study the performance of
		  various parallel substitution algorithms as well as an
		  alternative to substitution. In the alternative scheme,
		  substitution is replaced by very efficient distributed
		  matrix-vector multiplication at the expense of inverting
		  selected submatrices of the factor. Experiments on the
		  Intel Paragon demonstrate that the scheme using
		  matrix-vector multiplication has ideal scaled efficiency
		  for 1 to 128 processors and is significantly faster than
		  the traditional approach using the best parallel
		  substitution schemes.",
  isbn		= "978-1-4612-1516-5",
  doi		= "10.1007/978-1-4612-1516-5_13",
  note		= {\bibdoiurl{10.1007/978-1-4612-1516-5\_13}}
